{
    "name": "nevadskiy/laravel-tree",
    "description": "Tree-like structure for Eloquent models.",
    "license": "MIT",
    "keywords": ["tree", "hierarchy", "materialized path", "path", "ltree", "laravel", "eloquent"],
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.79|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.7|^7.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Tree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\Tree\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nevadskiy\\Tree\\TreeServiceProvider"
            ]
        }
    }
}
